{
  "name": "react-id-generator",
  "version": "3.0.0",
  "description": "Simple and universal HTML-id generator for React.",
  "repository": "https://github.com/Tomekmularczyk/react-id-generator.git",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "types": "./lib/index.d.ts",
  "scripts": {
    "dev": "concurrently \"yarn build:dev --watch\" \"yarn build:declarations --watch\"",
    "build:dev": "rollup -c",
    "build": "NODE_ENV=production rollup -c",
    "build:declarations": "tsc --declaration --emitDeclarationOnly --declarationDir lib",
    "lint": "eslint --ext .ts,.tsx .",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "keywords": [
    "id",
    "react",
    "react-id",
    "id-generator"
  ],
  "author": "Tomasz Mularczyk",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "concurrently": "^5.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "rollup": "^1.26.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint",
    "*.{js,ts,tsx,css,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
